{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "spacing": {
      "blockGap": null,
      "margin": true,
      "padding": true,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ]
    },
    "color": {
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#563500"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#c1a56c"
        }
      ]
    }
  }
}
